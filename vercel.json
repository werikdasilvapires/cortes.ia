{
  "version": 2,
  "builds": [
      {
      "src": "api/handler.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/upload.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/process.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/healthcheck.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api/handler.py" }
    { "src": "/api/upload", "dest": "api/upload.py" },
    { "src": "/api/process", "dest": "api/process.py" },
    { "src": "/api/healthcheck", "dest": "api/healthcheck.py" }
  ]
}